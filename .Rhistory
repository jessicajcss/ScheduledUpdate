AQI_Qualidade <- function(aqi) {
#                                     AQI
# Good                               0 - 50   |
# Moderate                          51 - 100  |
# Unhealthy for Sensitive Groups   101 â€“ 150  |
# Unhealthy                        151 â€“ 200  |
# Very Unhealthy                   201 â€“ 300  |
# Hazardous                        301 â€“ 500  |
if (aqi > 200) {
return("PÃ©ssima")  # Hazardous
} else if (aqi > 200) {
return("Muito Ruim")  # Very Unhealthy
} else if (aqi > 150) {
return("Ruim")   # Unhealthy
} else if (aqi > 100) {
return("Ruim para grupos sensÃ­veis")    # Unhealthy for Sensitive Groups
} else if (aqi > 50) {
return("Moderada")     # Moderate
} else if (aqi >= 0) {
return("Boa")            # Good
} else {
return(NA)
}
}
